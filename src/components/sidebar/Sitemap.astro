---
import sitemap from '../../../public/sitemap.json'
import CollapsibleTree from './CollapsibleTree.astro'
---

<div class="select-none border-r-2 border-zinc-950 max-md:border-none">
  {
    Object.entries(sitemap).map(([title, value]) => (
      <CollapsibleTree {title} {value} />
    ))
  }
</div>
